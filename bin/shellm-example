#!/usr/bin/env bash

## \brief An example cookiecutter-shellm project template.

include core/shellman.sh

[ $# -eq 0 ] && usage

main() {
  case "$1" in
    ## \option -h, --help
    ## Print this help and exit.
    -h|--help) shellman "$0"; exit 0 ;;
  esac
}

## \usage shellm-example [-h]
main "$@"
